doctype html
html
    head
    body
        h1= header
        form(action="/upload", method="post", enctype="multipart/form-data")
            input(type="file", name="myFile", accept="image/")
            input(type="submit", value="Upload Image", name="submit")
        div(id="image-container")
            each i in images
                img(src= `/uploads/${i}`)
                br
        script(src="polling.js")